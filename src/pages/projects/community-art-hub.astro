  <body>
    <NavBar />
    <section class="section template-banner">
      <div class="banner-full-image">
        <div class="background-banner"></div>
        <img
          alt="Interior Painting"
          loading="lazy"
          src="https://assets-global.website-files.com/65576d30478026e86cc17b29/656f15a43c7d6d278a3409be_siyan-peng-0j19szbmQO4-unsplash-p-1600.jpg"
