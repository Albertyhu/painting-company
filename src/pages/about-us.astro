    <link
      href="https://assets-global.website-files.com/65576d2f478026e86cc17ae3/6572db5aeb69948a923b80da_Favicon.png"
      rel="shortcut icon"
      type="image/x-icon"
    />
    <link
      href="https://assets-global.website-files.com/65576d2f478026e86cc17ae3/6572db65a9bef53b0c164ada_Webclip.png"
      rel="apple-touch-icon"
    />
  </head>
  <body>
    <NavBar />
    <InnerBanner />
    <OurMission />
    <WhyChooseUs />
    <OurExperts />
    <KeyPlayer />
    <ClientLogos />
    <FAQ />
    <ContactForm />
    <FooterComponent />
    <script
      src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=65576d2f478026e86cc17ae3"
      type="text/javascript"
      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"></script>
    <script src="./script/about.js" type="text/javascript"></script>
  </body>
</html>
